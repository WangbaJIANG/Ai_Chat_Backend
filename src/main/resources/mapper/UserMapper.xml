<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hclx.hclx_ai1.mapper.UserMapper">
    <insert id="insertUser" parameterType="com.hclx.hclx_ai1.entity.User">
        insert into user(id,username,password,nickname,gender)
        values (#{id},#{username},#{password},#{nickname},#{gender})
    </insert>


</mapper>
